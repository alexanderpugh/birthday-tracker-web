{
  "name": "birthday-tracker-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "dev": "concurrently \"npm run dev:server\" \"npm run serve\"",
    "dev:server": "nodemon lib/index.js",
    "start": "node lib/index.js",
    "heroku-postbuild": "npm run build"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.2",
    "bootstrap-vue": "^2.0.0-rc.2",
    "bunyan": "^1.8.12",
    "compression": "^1.7.2",
    "express": "^4.16.3",
    "helmet": "^3.12.0",
    "js-cookie": "^2.2.0",
    "vue": "^2.5.13",
    "vue-moment": "^3.2.0",
    "vue-router": "^3.0.1",
    "vuelidate": "^0.6.2",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0-beta.6",
    "@vue/cli-plugin-eslint": "^3.0.0-beta.6",
    "@vue/cli-service": "^3.0.0-beta.6",
    "concurrently": "^3.5.1",
    "nodemon": "^1.17.3",
    "vue-template-compiler": "^2.5.13"
  },
  "babel": {
    "presets": [
      "@vue/app"
    ]
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "env": {
      "browser": true,
      "node": true
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
